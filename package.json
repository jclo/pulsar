{
  "name": "@mobilabs/pulsar",
  "version": "1.0.2",
  "description": "A boilerplate to build complex Web Apps relying on Pakket",
  "main": "",
  "bin": {
    "pixarpulsar": "./bin/pixarpulsar.js"
  },
  "scripts": {
    "create": "node tasks/create.js",
    "build": "gulp build",
    "watch": "gulp watch",
    "dev": "gulp rundev",
    "test": "cross-env NODE_ENV=test nyc --reporter=lcov mocha --require esm ./test/main.js",
    "display-coverage": "opener ./coverage/lcov-report/index.html",
    "check-coverage": "nyc check-coverage --statements 40 --branches 20 --functions 40 --lines 40",
    "report-coverage": "nyc report --reporter=text-lcov | coveralls",
    "report": "nyc report",
    "makedist": "gulp makedist",
    "app": "gulp runapp",
    "prepare": "husky install",
    "doc": ""
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/jclo/pulsar.git"
  },
  "keywords": [
    "ES6"
  ],
  "author": {
    "name": "Mobilabs",
    "email": "contact@mobilabs.fr",
    "url": "http://www.mobilabs.fr"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jclo/pulsar/issues"
  },
  "homepage": "https://github.com/jclo/pulsar",
  "dependencies": {
    "@mobilabs/rview": "^1.0.10",
    "nopt": "^5.0.0",
    "shelljs": "^0.8.5"
  },
  "devDependencies": {
    "chai": "^4.3.5",
    "coveralls": "^3.1.1",
    "cross-env": "^7.0.3",
    "del": "^6.0.0",
    "eslint": "^7.32.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-plugin-import": "^2.25.4",
    "esm": "^3.2.25",
    "gulp": "^4.0.2",
    "gulp-clean-css": "^4.3.0",
    "gulp-concat": "^2.6.1",
    "gulp-connect": "^5.7.0",
    "gulp-header": "^2.0.9",
    "gulp-rename": "^2.0.0",
    "gulp-replace": "^1.1.3",
    "gulp-uglify-es": "^3.0.0",
    "html5-boilerplate": "^8.0.0",
    "husky": "^7.0.4",
    "jsdom": "^19.0.0",
    "mocha": "^8.4.0",
    "node-fetch": "^2.6.7",
    "nyc": "^15.1.0",
    "open": "^8.4.0",
    "opener": "^1.5.2",
    "pakket": "^1.0.6",
    "@mobilabs/es6pakket": "1.0.8"
  },
  "publishConfig": {
    "access": "public"
  },
  "private": false
